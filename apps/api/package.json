{
  "name": "@musicr/api",
  "version": "1.0.0",
  "description": "Backend API for Musicr chat application",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch --clear-screen=false src/index.ts",
    "build": "prisma generate && tsc && tsc-alias",
    "start": "node dist/index.js",
    "lint": "eslint src --ext .ts",
    "test": "vitest",
    "clean": "rm -rf dist",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "seed": "tsx scripts/seed.ts",
    "seed:simple": "tsx scripts/seed-simple.ts"
  },
  "dependencies": {
    "@fastify/cookie": "^9.4.0",
    "@fastify/cors": "^9.0.1",
    "@fastify/websocket": "^10.0.1",
    "@huggingface/transformers": "^3.7.3",
    "@prisma/client": "^5.11.0",
    "@xenova/transformers": "^2.17.2",
    "csv-parser": "^3.2.0",
    "dotenv": "^16.4.5",
    "fastify": "^4.26.2",
    "nanoid": "^5.1.6",
    "openai": "^4.28.4",
    "pino": "^8.19.0",
    "pino-pretty": "^11.0.0",
    "vader-sentiment": "^1.1.3",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/node": "^20.11.30",
    "@types/ws": "^8.18.1",
    "prisma": "^5.11.0",
    "tsc-alias": "^1.8.8",
    "tsx": "^4.7.1",
    "typescript": "^5.4.5",
    "vitest": "^1.4.0"
  }
}