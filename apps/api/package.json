{
  "name": "@musicr/api",
  "version": "1.0.0",
  "description": "Backend API for Musicr chat application",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch --clear-screen=false src/index.ts",
    "build": "prisma generate && tsc && tsc-alias",
    "start": "node dist/index.js",
    "start:railway": "prisma migrate deploy && node dist/index.js",
    "lint": "eslint src --ext .ts",
    "test": "vitest",
    "clean": "rm -rf dist",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:studio": "prisma studio",
    "seed": "tsx scripts/validate-catalog.ts && tsx scripts/seed.ts",
    "seed:simple": "tsx scripts/seed-simple.ts",
    "catalog:validate": "tsx scripts/validate-catalog.ts",
    "catalog:clean": "tsx scripts/clean-catalog.ts",
    "catalog:mb:fetch": "tsx scripts/ingestion/musicbrainz-genre-fetcher.ts",
    "catalog:mb:import": "tsx scripts/ingestion/musicbrainz-bulk-importer.ts",
    "catalog:embed": "tsx scripts/ingestion/embedding-backfill.ts",
    "catalog:safety": "tsx scripts/catalog-safety-check.ts",
    "catalog:stats": "tsx scripts/catalog-stats.ts",
    "db:jsonb:null": "tsx scripts/db-jsonb-null.ts"
  },
  "dependencies": {
    "@fastify/cookie": "^9.4.0",
    "@fastify/cors": "^9.0.1",
    "@fastify/websocket": "^10.0.1",
    "@huggingface/transformers": "^3.7.3",
    "@prisma/client": "^5.11.0",
    "@xenova/transformers": "^2.17.2",
    "csv-parse": "^6.1.0",
    "csv-parser": "^3.2.0",
    "csv-stringify": "^6.6.0",
    "csv-writer": "^1.6.0",
    "dotenv": "^16.4.5",
    "fastify": "^4.26.2",
    "ioredis": "^5.9.2",
    "nanoid": "^5.1.6",
    "openai": "^4.28.4",
    "pino": "^8.19.0",
    "pino-pretty": "^11.0.0",
    "prisma": "^5.11.0",
    "vader-sentiment": "^1.1.3",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/ioredis": "^5.0.0",
    "@types/node": "^20.11.30",
    "@types/ws": "^8.18.1",
    "tsc-alias": "^1.8.8",
    "tsx": "^4.7.1",
    "typescript": "^5.9.3",
    "vitest": "^1.4.0"
  }
}